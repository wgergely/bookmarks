QWidget {{
	background-color: {BG};
	color: {TEXT};
	border-width: 0;
	border: none;
	margin: 0;
	padding: 0;
	outline: 0;
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	selection-color: rgba(255,255,255,255);
	selection-background-color: rgba(250,250,255,50);
}}

QWidget:disabled {{
	color: {DISABLED_TEXT};
}}

QStackedWidget {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {SEPARATOR};
}}
QStackedWidget:focus {{
	border-color: {BLUE};
}}

QGroupBox {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	border: {HeightSeparator}px solid {TRANSPARENT_BLACK};
	border-radius: {RADIUS}px;
	background-color: {TRANSPARENT_BLACK};
}}
QGroupBox::title {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeSmall}px;
	color: {SECONDARY_TEXT};
  subcontrol-origin: margin;
  subcontrol-position: top center; /* position at the top center */
  margin: 0 {RADIUS}px;
}}
QGroupBox:focus {{
	border: {HeightSeparator}px solid {BLUE};
}}

QSplitter:handle {{
	border: {HeightSeparator}px solid {TRANSPARENT_BLACK};
}}

QStatusBar {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeSmall}px;
	color: {TEXT};
	background-color: {SEPARATOR};
	padding: {RADIUS}px;
	margin: {RADIUS}px;
}}
QStatusBar::item {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	text-align: center;
	padding: {RADIUS}px;
	margin: {RADIUS}px;
	border: none;
}}


QCheckBox {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	height: {FontSizeLarge}px;
	border-radius: {RADIUS}px;
	background-color: {DARK_BG};
	margin: 0;
	padding: {RADIUS_SM}px;
	color: {SECONDARY_TEXT};
}}
QCheckBox::indicator {{
	height: {FontSizeMedium}px;
	width: {FontSizeMedium}px;
	border-radius: {RADIUS}px;
}}
QCheckBox::indicator:unchecked {{
	/* image: url({UNCHECKED}); */
	background-color: {SEPARATOR};
}}
QCheckBox::indicator:checked  {{
	/* image: url({CHECKED}); */
	background-color: {GREEN};
}}
QCheckBox:checked  {{
	color: {SELECTED_TEXT};
}}


QRadioButton {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	color: {SECONDARY_TEXT};
	background-color: {TRANSPARENT};
	margin: 0;
	padding: {RADIUS}px;
}}
QRadioButton:disabled {{
	color: {DISABLED_TEXT};
	background-color: {DARK_BG};
}}
QRadioButton::indicator {{
	height: {FontSizeMedium}px;
	width: {FontSizeMedium}px;
	border: none;
	border-radius: {HeightSeparator}px;
}}
QRadioButton::indicator:unchecked {{
	background-color: {DARK_BG};
}}
QRadioButton::indicator:checked  {{
	background-color: {SECONDARY_TEXT};
}}


QPushButton {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	color: {TEXT};
	text-align: center;
	padding-left: {FontSizeMedium}px;
	padding-right: {FontSizeMedium}px;
	border-style: none;
	min-height: {SMALL_ROW_HEIGHT}px;
	background-color: {SELECTED_BG};
	border-radius: {RADIUS_SM}px;
}}
QPushButton:disabled {{
	color: {SECONDARY_TEXT};
	background-color: {DARK_BG};
	border: {HeightSeparator}px solid {DARK_BG};
}}
QPushButton:hover {{
	color: {SELECTED_TEXT};
	background-color: {SELECTED_BG};
	border: {HeightSeparator}px solid {SELECTED_BG};
}}
QPushButton:pressed {{
	color: {SELECTED_TEXT};
	background-color: {DARK_BG};
	border: {HeightSeparator}px solid {DARK_BG};
}}
QPushButton:focus {{
	border: {HeightSeparator}px solid {BLUE};
}}


QComboBox {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	color: {TEXT};
	text-align: left;
	padding-left: {FontSizeMedium}px;
	padding-right: {FontSizeMedium}px;
	border-style: solid;
	border: {HeightSeparator}px solid {TRANSPARENT_BLACK};
	background-color: {DARK_BG};
	height: {SMALL_ROW_HEIGHT}px;
	border-radius: {RADIUS}px;
}}
QComboBox:focus {{
	border: {HeightSeparator}px solid {BLUE};
}}

QComboBox::drop-down {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	width: 0;
	height: 0;
	padding: 0;
	margin: {HeightSeparator}px;
	border-width: 0;
	border-style: none;
	border-radius: {RADIUS_SM}px;
}}
QComboBox QListView::item {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	color: {TEXT};
	height: {SMALL_ROW_HEIGHT}px;
	min-height: {SMALL_ROW_HEIGHT}px;
	background-color: {DARK_BG};
	padding-left: {RADIUS_SM}px;
}}
QComboBox QListView::item:hover {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	color: {SELECTED_TEXT};
	background-color: {SELECTED_BG};
}}

QComboBox:hover {{
		color: {SELECTED_TEXT};
}}
QComboBox:disabled {{
	color: {DISABLED_TEXT};
	background-color: {DARK_BG};
}}
QComboBox:on {{
	color: {DISABLED_TEXT};
	background-color: {TRANSPARENT_BLACK};
}}


QLineEdit {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {DARK_BG};
	border: none;
	min-height: {SMALL_ROW_HEIGHT}px;
	border-radius: {HeightSeparator}px;
	padding: {RADIUS}px;
}}
QLineEdit:disabled {{
	background-color: {DARK_BG};
	color: {DISABLED_TEXT};
}}

.QTextEdit {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {DARK_BG};
	border: none;
	border-radius: {HeightSeparator}px;
	padding: {RADIUS}px;
}}
.QTextEdit:disabled {{
	background-color: {DARK_BG};
	color: {DISABLED_TEXT};
}}

QScrollBar {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background: {SEPARATOR};
	width: {SCROLLBAR_SIZE}px;
	height: {SCROLLBAR_SIZE}px;
}}
QScrollBar::handle {{
	background: {DARK_BG};
	border: none;
	min-height: {SCROLLBAR_MINHEIGHT}px;
}}

QScrollBar::add-page,
QScrollBar::add-line,
QScrollBar::sub-page,
QScrollBar::up-arrow,
QScrollBar::down-arrow,
QScrollBar::sub-line {{
	background: {SEPARATOR};
}}


QTableWidget,
QListWidget,
QListView {{
	border-radius: {RADIUS}px;
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background: {DARK_BG};
}}
QTableWidget::item,
QListWidget::item,
QListView::item {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	outline: 0;
	color: {TEXT};
  background: {BG};
	border-bottom: {HeightSeparator}px solid {TRANSPARENT_BLACK};
}}
QTableWidget::item:hover,
QListWidget::item:hover,
QListView::item:hover {{
  background: {SELECTED_BG};
}}
QTableWidget::item:selected,
QListWidget::item:selected,
QListView::item:selected {{
	background: {SELECTED_BG};
}}
QTableWidget::item:selected:hover,
QListWidget::item:selected:hover,
QListView::item:selected:hover {{
	background: {SELECTED_BG};
}}
QTableWidget::item:disabled,
QListWidget::item:disabled,
QListView::item:disabled {{
  color: {DISABLED_TEXT};
	background: {TRANSPARENT};
}}
QTableWidget::indicator,
QListWidget::indicator,
QListView::indicator {{
	width: {FontSizeMedium}px;
	height: {FontSizeMedium}px;
	border-radius: {RADIUS}px;
	background-color: {DARK_BG};
	margin-left: {RADIUS}px;
	margin-right: {RADIUS}px;
	subcontrol-position: center;
}}
QTableWidget::indicator:checked,
QListWidget::indicator:checked,
QListView::indicator:checked {{
	background: {GREEN};
}}

TableWidget {{
	background: {TRANSPARENT};
	background-color: {TRANSPARENT};
	selection-background-color: {TRANSPARENT};
}}
TableWidget::item:selected,
TableWidget::item:hover,
TableWidget::item:disabled,
TableWidget::item {{
		padding: {WidthIndicator}px;
		background-color: {TRANSPARENT};
		background: {TRANSPARENT};
}}

QHeaderView {{
    font-family: "{PRIMARY_FONT}";
	border: none;
	border-style: none;
}}
QHeaderView::section {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeSmall}px;
	background-color: {TRANSPARENT};
	alignment: center;
	text-align: center;
	color: {SECONDARY_TEXT};
	border: none;
	border-style: none;
}}


QTreeView {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {DARK_BG};
	border: none;
}}
QTreeView::item {{
	border: none;
}}
QTreeView::item:hover {{
  background: {BG};
}}
QTreeView::item:focus {{
  background: {TRANSPARENT};
}}
QTreeView::item:selected {{
	background: {SELECTED_BG};
}}
QTreeView:branch {{
	background-color: {DARK_BG};
	border: none;
	border-image: none;
}}
QTreeView::branch:focus {{
  background: {TRANSPARENT};
}}
QTreeView:branch:selected {{
	background: {SELECTED_BG};
}}
QTreeView:branch:!open:has-children {{
  image: url("{BRANCH_CLOSED}");
}}
QTreeView:branch:open:has-children:!has-siblings,
QTreeView:branch:open:has-children:has-siblings {{
  image: url("{BRANCH_OPEN}");
}}

QLabel {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {TRANSPARENT};
}}
QLabel:disabled {{
	background-color: {TRANSPARENT};
	color: {DISABLED_TEXT};
}}

QMenu {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
  background-color: {DARK_BG};
	menu-scrollable: 1;
	icon-size: {CONTEXT_MENU_HEIGHT}px;
}}
QMenu::separator {{
	height: {HeightSeparator}px;
	margin-top: {WidthIndicator}px;
	margin-bottom: {WidthIndicator}px;
  background-color: {TRANSPARENT_BLACK};
  color: {TRANSPARENT};
	min-width: 1000px;
}}
QMenu::item {{
	height: {CONTEXT_MENU_HEIGHT}px;
	margin: 0px;
	padding-right: {WidthMargin}px;
	padding-left: {WidthMargin}px;
}}
QMenu::icon {{
	padding: {CONTEXT_MENU_ICON_PADDING}px;
}}

QMenu::item:selected {{
	color: {SELECTED_TEXT};
  background-color: {SELECTED_BG};
}}
QMenu::item:disabled {{
	color: {DISABLED_TEXT};
  background-color: {TRANSPARENT_BLACK};
}}
QMenu::right-arrow {{
  margin: 0;
	padding-right: {WidthMargin}px;
  image: url({BRANCH_CLOSED});
  height: {WidthMargin}px;
  width: {WidthMargin}px;
}}
QMenu::keystroke {{
  color: red;
}}
QMenu::shortcut {{
  color: red;
}}

QToolTip {{
	opacity: 200;
	font-family: "{SECONDARY_FONT}";
	font-size: {FontSizeSmall}px;
  background-color: {DARK_BG};
	padding: {RADIUS}px;
	color: {SECONDARY_TEXT};
	border: {HeightSeparator}px solid {SEPARATOR};
}}

CheckBoxButton,
DragIndicatorButton {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {TEXT};
	margin: 0 0 0 0;
	padding: 0 0 0 0;
}}
CheckBoxButton:disabled,
DragIndicatorButton:disabled {{
	background-color: {TRANSPARENT};
}}

TodoEditors,
TodoItemWidget,
TodoItemEditor {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	color: {DARK_BG};
	background-color: {TRANSPARENT};
	border-radius: {RADIUS}px;
	border: {HeightSeparator}px solid {TRANSPARENT};
}}
TodoEditors:disabled,
TodoItemWidget:disabled,
TodoItemEditor:disabled {{
	color: {GREEN};
}}
TodoItemEditor:focus {{
	border: {HeightSeparator}px solid {TRANSPARENT_BLACK};
}}

TodoEditorWidget QScrollBar {{
	background: {TRANSPARENT};
	border: none;
	border-radius: 0;
	width:{RADIUS}px;
	height: {RADIUS}px;
}}
TodoEditorWidget QScrollBar:vertical,
TodoEditorWidget QScrollBar:horizontal {{
	background: {TEXT};
	border: none;
}}
TodoEditorWidget QScrollBar::handle {{
	background: {SECONDARY_TEXT};
	border: none;
	border-radius: {RADIUS}px;
	border: none;
	max-height: 250;
	min-height: {WidthMargin}px;
}}

TodoEditorWidget QScrollBar::add-page,
TodoEditorWidget QScrollBar::sub-page,
TodoEditorWidget QScrollBar::add-line,
TodoEditorWidget QScrollBar::up-arrow,
TodoEditorWidget QScrollBar::down-arrow,
TodoEditorWidget QScrollBar::sub-line {{
	background: {TEXT};
	border: none;
	border-radius: 0;
}}

LineEdit,
LineEdit:disabled {{
    background-color: {TRANSPARENT};
    font-family: "{PRIMARY_FONT}";
    font-size: {FontSizeMedium}px;
    border-bottom: {HeightSeparator}px solid {TRANSPARENT};
    border-radius: 0;
    color: {TEXT};
}}

LineEdit:disabled {{
	color: {DISABLED_TEXT};
}}


LineEdit:!read-only:focus{{
    border-bottom: {HeightSeparator}px solid {GREEN};
}}

.ListWidget {{
	border-radius: {RADIUS}px;
	border: {HeightSeparator}px solid {DARK_BG};
	border-radius: {RADIUS}px;
}}
.ListWidget:focus {{
		border: {HeightSeparator}px solid {BLUE};
}}

BasePropertyEditor,
BasePropertyEditor > QWidget,
BasePropertyEditor > QScrollArea,
BasePropertyEditor > QScrollArea > QWidget,
BasePropertyEditor > QScrollArea > QWidget > QWidget {{
	background: {BG};
	background-color: {BG};
}}

MainWidget  {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {SEPARATOR};
}}
MayaWidget {{
	font-family: "{PRIMARY_FONT}";
	font-size: {FontSizeMedium}px;
	background-color: {SEPARATOR};
}}

QWidget#mainRow {{
	background-color: {DARK_BG};
}}

QProgressBar {{
	border: {HeightSeparator}px solid {SEPARATOR};
	border-radius: {RADIUS}px;
	background-color: {DARK_BG};
	text-align: center;
}}

QProgressBar::chunk {{
    background-color: {GREEN};
    width: {WidthIndicator}px;
		margin: 0;
}}

QTableView {{
    gridline-color: {SEPARATOR};
}}

QTabWidget::pane {{ /* The tab widget frame */
    border-top: {HeightSeparator}px solid {TRANSPARENT_BLACK};
}}

QTabWidget::tab-bar {{
    left: {RADIUS}px;
    padding: {WidthIndicator}px;
}}

QTabBar::tab {{
    background: {TRANSPARENT_BLACK};
    color: {DISABLED_TEXT};
    border: none;
    border-style: none;
    border-top-left-radius: {RADIUS}px;
    border-top-right-radius: {RADIUS}px;
    padding: {RADIUS}px;
    margin-left:{HeightSeparator}px;
    margin-right:{HeightSeparator}px;
}}
QTabBar::tab:selected, QTabBar::tab:hover {{
    color: {SELECTED_TEXT};
    background: {BG};
}}
