{
  "_metadata": {
    "major_version": 1,
    "minor_version": 1
  },
  "display_information": {
    "name": "Bookmarks Bot",
    "description": "Bookmarks Slack Bot",
    "background_color": "#ffffff"
  },
  "features": {
    "bot_user": {
      "display_name": "Bookmarks Bot",
      "always_online": false
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "channels:history",
        "channels:read",
        "chat:write",
        "chat:write.public",
        "groups:read",
        "users:read",
        "im:write",
        "im:history"
      ]
    }
  },
  "settings": {
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}
